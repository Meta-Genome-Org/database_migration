<!--======================================================================-->
<!--                Creating types for stress-strain-ISO                  --> 
<!--======================================================================-->

    <xsd:complexType name="stress-strain-points-iso">
        <xsd:sequence>
            <xsd:annotation>
                <xsd:appinfo>
                    <label>Stress strain data</label>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="unbounded" name="stress-strain-topology-iso" type="topology-upload">
                <xsd:annotation>
                <xsd:appinfo>
                    <label>Metamaterial topology upload of individual parts for stress-strain data</label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:element>
        <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="stress-strain-mesh-iso" type="mesh-upload">
                <xsd:annotation>
                <xsd:appinfo>
                    <label>Metamaterial mesh upload for stress-strain data</label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="stress-strain-unit-iso">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Stress units</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="KPa"/>
                        <xsd:enumeration value="MPa"/>
                        <xsd:enumeration value="GPa"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Choose to upload .xls (excel 97-2003) file or manually input data points</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" type="xls-upload-stress-strain" name="stress-strain-xls-iso">
                    <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                            upload Excel workbook/spreadsheet
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
                </xsd:element>
                <xsd:element form="unqualified" minOccurs="1" maxOccurs="unbounded" type="stress-strain-point" name="stress-strain-input-iso"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

