    <xsd:complexType name="component-indentor-xls-upload">
        <xsd:sequence>
            <xsd:element name="fileExtension" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>File extension</label>
                        <placeholder>e.g. .xls</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="xls-upload-component-table" type="Table">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Please upload Excel 97-2003 Workbook .xls file for stress strain data.
                        .xls file MUST contain time data (s) in column A, displacement in column B and force data (N) data in column C only WITH row 1 blank.
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="time-confirm" form="unqualified" minOccurs="1" maxOccurs="1" type="select-yes-no">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Confirm column A is time data (s) 
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="displacement-confirm" form="unqualified" minOccurs="1" maxOccurs="1" type="select-yes-no">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Confirm column B is displacement data
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="force-confirm" form="unqualified" minOccurs="1" maxOccurs="1" type="select-yes-no">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Confirm column C is force data (N)
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="component-impactor-xls-upload">
        <xsd:sequence>
            <xsd:element name="fileExtension" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>File extension</label>
                        <placeholder>e.g. .step, .stl, .cad</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="xls-upload-component-table" type="Table">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Please upload Excel 97-2003 Workbook .xls file for stress strain data.
                        .xls file MUST contain time data (s) in column A, depth in column B and force data (N) data in column C only WITH row 1 blank.
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="time-confirm" form="unqualified" minOccurs="1" maxOccurs="1" type="select-yes-no">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Confirm column A is time data (s) 
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="depth-confirm" form="unqualified" minOccurs="1" maxOccurs="1" type="select-yes-no">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Confirm column B is depth data
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="force-confirm" form="unqualified" minOccurs="1" maxOccurs="1" type="select-yes-no">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Confirm column C is force data (N)
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

