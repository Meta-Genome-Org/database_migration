<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<xsd:schema attributeFormDefault="unqualified" elementFormDefault="unqualified"
    xmlns:ds="http://datacite.org/schema/kernel-4" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    

    <xsd:complexType name="attribute-base"> </xsd:complexType>

<!--======================================================================-->
<!--        Creating fields for general metamaterial information          --> 
<!--======================================================================-->
    <xsd:complexType name="general-metamaterial-info">
        
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="unbounded" name="family">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>metamaterial family</label>
                        <documentation>This block should pass meta data to the parser</documentation>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Foam"/>
                        <xsd:enumeration value="Textile"/>
                        <xsd:enumeration value="Fibre reinforced composite"/>
                        <xsd:enumeration value="Idealised lattice"/>
                        <xsd:enumeration value="Honeycomb"/>
                        <xsd:enumeration value="Kirigami"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>

            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="unusual_property">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Unusual material property</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="negative Poissons ratio"/>
                        <xsd:enumeration value="negative stiffness"/>
                        <xsd:enumeration value="negative compressability"/>
                        <xsd:enumeration value="force/torque coupling"/>
                        <xsd:enumeration value="negative moisture expansion"/>
                        <xsd:enumeration value="negative thermal expansion"/>
                        <xsd:enumeration value="other"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>

            <xsd:element form="unqualified" name="description" minOccurs="0" maxOccurs="1" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <module>module-text-area</module>
                        <label>Metamaterial description</label>
                        <placeholder>Please provide a short description of your metamaterial here.</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="num_base_mats" type="xsd:integer">
                <xsd:annotation>
                        <xsd:appinfo>
                            <label>number of base material components</label>
                            <placeholder>E.g. 1, 2, 3...</placeholder>
                        </xsd:appinfo>
                    </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

<!--======================================================================-->
<!--                Creating type for testing conditions                  --> 
<!--======================================================================-->
    <xsd:complexType name="test-conditions">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="test-temperature" type="xsd:float">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Test temperature</label>
                        <placeholder>E.g. 273.15</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="testing-temperature-units">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Testing temperature units</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="&#730;C"/>
                        <xsd:enumeration value="&#730;K"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="relative-humidity" type="xsd:float">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Relative humidity (%)</label>
                        <placeholder>E.g. 60</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="loading-rate" type="xsd:float">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Test loading rate</label>
                        <placeholder>E.g. 0.1</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="loading-rate-unit">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Loading rate units</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="cm/s"/>
                        <xsd:enumeration value="mm/s"/>
                        <xsd:enumeration value="%/s"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

<!--======================================================================-->
<!--             Creating type for capturing xls - stress strain          --> 
<!--======================================================================-->
    
    <xsd:complexType xmlns:ns0="http://mdcs.ns" name="Table" ns0:_mod_mdcs_="/diffusion/upload-excel">
        <xsd:annotation>
            <xsd:appinfo>
                <module>module-excel-uploader</module>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="headers" type="Headers"/>
            <xsd:element name="rows" type="Rows"/>
        </xsd:sequence>
        <xsd:attribute name="name" type="xsd:string" use="optional"/>
    </xsd:complexType>

    <xsd:complexType name="Headers">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="column" type="Column"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="Rows">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="row" type="Row"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="Row">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="column" type="Column"/>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:string" use="required"/>
    </xsd:complexType>
    <xsd:complexType name="Column">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute name="id" type="xsd:string" use="required"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="xls-upload-stress-strain">
        <xsd:sequence>
            <xsd:element name="fileExtension" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>File extension</label>
                        <placeholder>e.g. .step, .stl, .cad</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="xls-upload-stress-strain-table" type="Table">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Please upload Excel 97-2003 Workbook .xls file for stress strain data.
                        .xls file MUST contain strain data (%) in column A and stress data in column B only WITH row 1 blank. (compressive stress and compressive strain must be negative)
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="strain-confirm" form="unqualified" minOccurs="1" maxOccurs="1" type="select-yes-no">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Confirm column A is strain data (%) 
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="stress-confirm" form="unqualified" minOccurs="1" maxOccurs="1" type="select-yes-no">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Confirm column B is stress data
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="select-yes-no">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="selection">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Confirmation</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="select"/>
                        <xsd:enumeration value="yes"/>
                        <xsd:enumeration value="no"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


<!--======================================================================-->
<!--        Creating type for capturing xls - ax_strain-t_strain          --> 
<!--======================================================================-->
    <xsd:complexType name="xls-upload-ax_strain-t_strain">
        <xsd:sequence>
            <xsd:element name="fileExtension" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>File extension</label>
                        <placeholder>e.g. .step, .stl, .cad</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="xls-upload-ax_strain-t_strain-table" type="Table">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                            Please upload Excel 97-2003 Workbook .xls file for axial strain vs transverse strain data.
                            .xls file MUST contain axial strain data (%) in column A and transverse strain data (%) data in column B only WITH row 1 blank.(compressive strain must be negative)
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="ax-strain-confirm" form="unqualified" minOccurs="1" maxOccurs="1" type="select-yes-no">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Confirm column A is axial strain data (%) 
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="trans-strain-confirm" form="unqualified" minOccurs="1" maxOccurs="1" type="select-yes-no">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Confirm column B is transverse strain data (%)
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

