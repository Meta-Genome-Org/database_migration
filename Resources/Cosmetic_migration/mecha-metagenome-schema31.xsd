                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Time unit</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="s"/>
                        <xsd:enumeration value="ms"/>
                        <xsd:enumeration value="&#181;s"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="depth-units" form="unqualified" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Depth unit</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="m"/>
                        <xsd:enumeration value="mm"/>
                        <xsd:enumeration value="&#181;m"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="force-units" form="unqualified" minOccurs="1" maxOccurs="1">
