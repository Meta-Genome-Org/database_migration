                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="submitter-last-name" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Submitter last name</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="submitter-email" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Submitter email address</label>
                        <placeholder>jsmith@place.gov</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="publication-details">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="id" type="xsd:anyURI">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication DOI</label>
                        <placeholder>E.g. http://dx.doi.org/123/abc</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="unbounded" name="publication-authors" type="authors"/>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="publication-title" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication title</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="publication-year" type="xsd:integer">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication year</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="publication-journal" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication journal</label>
                        <placeholder>e.g. Nature</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="publication-volume" type="xsd:integer">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication volume</label>
                        <placeholder>e.g. 1</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="publication-issue" type="xsd:integer">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication issue</label>
                        <placeholder>e.g. 1</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="publication-page" type="xsd:integer">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication page</label>
                        <placeholder>e.g. 1</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="publication-url" type="xsd:anyURI">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Publication url</label>
                        <placeholder>e.g. nature.com/mypublicaton</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="Publication-submitter" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Submitter name</label>
                        <placeholder>Bill Smith</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="Publication-submitter-email" type="emailAddress">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Submitter email</label>
                        <placeholder></placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="emailAddress"> 
        <xsd:restriction base="xsd:string"> 
            <xsd:pattern value="[^@]+@[^\.]+\..+"/> 
        </xsd:restriction> 
    </xsd:simpleType> 

    <xsd:complexType name="authors">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="author-initials" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Author initial</label>
                        <placeholder>e.g. A., A. B., A. B. C.,</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="author-surname" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Author Surname</label>
                        <placeholder>e.g. Feynman</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

<!--======================================================================-->
<!--                Creating fields for stress=strain convention          -->
<!--======================================================================-->

    <xsd:complexType name="stress-strain-convention">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="strain-convention">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Strain convention</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Engineering Strain"/>
                        <xsd:enumeration value="True Strain"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="stress-convention">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Stress convention</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Engineering Stress"/>
                        <xsd:enumeration value="True Stress"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


<!--======================================================================-->
<!--         Torsional + shear + uniaxial dynamic component test          -->
<!--======================================================================-->

    <xsd:complexType name="torsional-component">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="torsional-data" type="tors-type">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Torsional test data
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="shear-component">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="shear-data" type="shear-type">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Shear test data
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="uniax_dynam-component">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="uniax_dynam-data" type="uniax_dynam-type">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Uniaxial dynamic test data
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="tors-type">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="unbounded" name="torsion-test-topology" type="topology-upload">
                <xsd:annotation>
                <xsd:appinfo>
                    <label>Metamaterial topology upload of individual parts for torsion test</label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:element>
        <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="torsion-test-mesh" type="mesh-upload">
                <xsd:annotation>
                <xsd:appinfo>
                    <label>Metamaterial mesh upload for torsion test data</label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:element>
            <xsd:element name="torsional-axis-of-rotation" form="unqualified" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Axis of rotation</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="x"/>
                        <xsd:enumeration value="y"/>
                        <xsd:enumeration value="z"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="polar-second-moment-area" form="unqualified" minOccurs="1" maxOccurs="1" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Polar second moment of area 
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="polar-second-moment-area-unit">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Polar second moment of area Unit</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="m&#8308;"/>
                        <xsd:enumeration value="mm&#8308;"/>
                        <xsd:enumeration value="&#181;m&#8308;"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="tors-sample-height">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Sample height</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="tors-sample-height-units">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Sample height unit</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="m"/>
                        <xsd:enumeration value="mm"/>
                        <xsd:enumeration value="&#181;m"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="fileExtension" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>File extension</label>
                        <placeholder>e.g. .step, .stl, .cad</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="xls-upload-tors-sher-uniax_dynam-table" type="Table">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Please upload Excel 97-2003 Workbook .xls file for stress strain data.
                        .xls file MUST contain time data (s) in column A, and angle data (degrees) in column B and torque data (Newton-metre) in column C only WITH row 1 blank. (compressive stress and compressive strain must be negative)
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="shear-type">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="shear-test-topology" type="topology-upload">
                <xsd:annotation>
                <xsd:appinfo>
                    <label>Metamaterial topology upload of individual parts for shear test</label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:element>
        <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="shear-test-mesh" type="mesh-upload">
                <xsd:annotation>
                <xsd:appinfo>
                    <label>Metamaterial mesh upload for shear test data</label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:element>
            <xsd:element name="shear-loading-axis" form="unqualified" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Loading axis</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="x"/>
                        <xsd:enumeration value="y"/>
                        <xsd:enumeration value="z"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="shear-loaded-face" form="unqualified" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Loaded face</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="x"/>
                        <xsd:enumeration value="y"/>
                        <xsd:enumeration value="z"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="fileExtension" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>File extension</label>
                        <placeholder>e.g. .step, .stl, .cad</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="xls-upload-shear-table" type="Table">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Please upload Excel 97-2003 Workbook .xls file for stress strain data.
                        .xls file MUST contain time data (s) in column A, and strain data in column B and stress data in column C only WITH row 1 blank. (compressive stress and compressive strain must be negative)
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="uniax_dynam-type">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="uniax_dynam-test-topology" type="topology-upload">
                <xsd:annotation>
                <xsd:appinfo>
                    <label>Metamaterial topology upload of individual parts for uniaxial dynamic test</label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:element>
        <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="uniax_dynam-mesh" type="mesh-upload">
                <xsd:annotation>
                <xsd:appinfo>
                    <label>Metamaterial mesh upload for uniaxial dynamic data</label>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:element>
            <xsd:element name="loading-axis" form="unqualified" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Loading axis</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="x"/>
                        <xsd:enumeration value="y"/>
                        <xsd:enumeration value="z"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="fileExtension" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>File extension</label>
                        <placeholder>e.g. .step, .stl, .cad</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="xls-upload-uniax_dynam-table" type="Table">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>
                        Please upload Excel 97-2003 Workbook .xls file for stress strain data.
                        .xls file MUST contain time data (s) in column A, and strain data in column B and stress data in column C only WITH row 1 blank. (compressive stress and compressive strain must be negative)
                        </label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>



<!--======================================================================-->
<!--                    identify bulk material ID and name                -->
<!--======================================================================-->

    <xsd:complexType name="base-material-identity">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="base-material-class">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Material Classification</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Metallic"/>
                        <xsd:enumeration value="Ceramic"/>
                        <xsd:enumeration value="Organic"/>
                        <xsd:enumeration value="Inorganic"/>
                        <xsd:enumeration value="Other"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="new-base-class" type="xsd:string">
                <xsd:annotation>
                        <xsd:appinfo>
                            <label>If other, please specify here</label>
                        </xsd:appinfo>
                    </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="base-material-id" type="xsd:anyURI">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Meta-genome base material PID</label>
                        <placeholder>e.g. https://portal.meta-genome.org/pid/rest/local/cdcs/BXN3Y3GZJ6U6ICAV</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


<!--======================================================================-->
<!--                    identify meta-material ID and name                -->
<!--======================================================================-->

<xsd:complexType name="meta-material-identity">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="meta-material-name" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Name of metamaterial</label>
                        <placeholder>e.g. chiral_35</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="meta-material-id" type="xsd:integer">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Meta-genome metamaterial id</label>
                        <placeholder>e.g. 8</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


<!--======================================================================-->
<!--                        Developer section                             -->
<!--======================================================================-->

    <xsd:complexType name="dev-section">
        <xsd:annotation>
                    <xsd:appinfo>
                        <placeholder>Developer section - please leave blank</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="pid">
                <xsd:annotation>
                    <xsd:appinfo>
                        <placeholder>Leave blank - will auto generate</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="up-to-date-check" type="is-up-to-date">
                <xsd:annotation>
                    <xsd:appinfo>
                        <placeholder>Is this the most up to date version of the data?</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="is-up-to-date">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="dev-most-recent-check">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Is this the most up to date version of the data?</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Select"/>
                        <xsd:enumeration value="Yes"/>
                        <xsd:enumeration value="No"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="most-up-to-date-pid" type="xsd:anyURI">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>If no, please provide Most recent PID</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="up-to-date-pid">
        <xsd:sequence>
            <xsd:element form="unqualified" name="pid-url" minOccurs="1" maxOccurs="1" type="xsd:anyURI">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Please provide the Meta-genome PID for the most recent version</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


<!--======================================================================-->
<!--                        Version control                               -->
<!--======================================================================-->


    <xsd:complexType name="version-control">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="cc-license-agreement">
                    <xsd:annotation>
                            <xsd:appinfo>
                                <label>Do you accept that your data is contributed under CC SY license? https://creativecommons.org/licenses/</label>
                            </xsd:appinfo>
                    </xsd:annotation>
                    <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Select"/>
                        <xsd:enumeration value="Yes"/>
                        <xsd:enumeration value="No"/>
                    </xsd:restriction>
                </xsd:simpleType>
                </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="is-updating-version">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Are you updating a previous submission?</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Select"/>
                        <xsd:enumeration value="Yes"/>
                        <xsd:enumeration value="No"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
                <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="previous-submission-data" type="previous-submission-info">
                    <xsd:annotation>
                            <xsd:appinfo>
                                <label>If yes, please provide information:</label>
                            </xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="previous-submission-info" >
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="previous-submission-pid" type="xsd:anyURI">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Meta-genome PID of submission being updated</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" name="description" minOccurs="1" maxOccurs="1" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <module>module-text-area</module>
                        <label>Information and context for submission update</label>
                        <placeholder>Please provide a short description of your metamaterial here.</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

<!--======================================================================-->
<!--          CALLING ALL FIELDS AND COMPONENTS - MECH GENOME             -->
<!--======================================================================-->

    <xsd:complexType name="mech-genome-map">
        <xsd:sequence>

            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="versioning" type="version-control">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Version control</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="publication-info" type="publication-details">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Reference information</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
                    
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="stress-strain-convention-info" type="stress-strain-convention">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Mechanical convention used throughout</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <xsd:choice minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Data type</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:element form="unqualified" minOccurs="0" maxOccurs="0" name="Select">
                    <xsd:annotation>
                            <xsd:appinfo>
                                <label>Select</label>
                            </xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" type="base-material-properties" name="base-material-info">
                <xsd:annotation>
                            <xsd:appinfo>
                                <label>bulk material</label>
                            </xsd:appinfo>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="metamaterial-material-info" type="metamaterial-properties" >
                    <xsd:annotation>
                    <xsd:appinfo>
                        <label>meta-material</label>
