                        <xsd:enumeration value="KN"/>
                        <xsd:enumeration value="N"/>
                        <xsd:enumeration value="mN"/>
                        <xsd:enumeration value="&#181;N"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:appinfo>
