            <xsd:element form="unqualified" minOccurs="0" maxOccurs="1" name="param-model" type="xsd:string">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>reference of model</label>
                        <placeholder>Gibson, L. J. and Ashby, M. F. (1997) Cellular solids. Structure and properties. Cambridge: Press Syndicate of the University of Cambridge. 2nd Edition. pp. 101-106</placeholder>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element> 
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="metamaterial-foam-params">
