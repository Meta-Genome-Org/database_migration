    <xsd:complexType name="metamaterial-foam-auxetic-params">
        <xsd:sequence>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="lcr-z" type="xsd:float">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Linear compression ratio z</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="lcr-x" type="xsd:float">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Linear compression ratio x</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="lcr-x" type="xsd:float">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Linear compression ratio y</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="final-vol-ratio" type="xsd:float">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Final volume ratio</label>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element form="unqualified" minOccurs="1" maxOccurs="1" name="lcr-convention">
                <xsd:annotation>
                    <xsd:appinfo>
                        <label>Linear compression ratio convention</label>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="original/final"/>
                        <xsd:enumeration value="final/original"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="metamaterial-custom-params">
        <xsd:sequence>
            <xsd:annotation>
